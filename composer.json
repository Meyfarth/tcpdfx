{
    "name": "opichon/tcpdf",
    "description": "Fork of the unofficial PSR-0 compliant version of the FPDI library. Designed to be used with TCPDF.",
    "type": "library",
    "require": {
        "php": ">=5.3.0",
        "tcpdf/tcpdf": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "fpdi": "lib/",
            "tcpdf": "lib/"
        },
        "classmap": [
          "data/fonts",
          "config"
        ]
    }
}
